		"yauzl": "2.10.0"
	},
	"nyc": {
		"check-coverage": true,
		"all": true,
		"cache": true,
		"lines": 80,
		"statements": 80,
		"functions": 80,
		"branches": 80,
		"extension": [
			".js"
		],
		"include": [
			"build/app/**/*.js"
		],
		"exclude": [
			"build/app/files/processor/processor.js",
			"build/app/common/settings.validator.js",
			"build/app/common/middleware/website.middleware.js",
			"build/app/common/profiler.js",
			"build/app/authentication/strategies/**",
			"build/app/vp/**",
			"build/app/vpinball/**"
		],
		"reporter": [
			"lcov",
			"html",
			"text-summary"
		]
	},
	"snyk": true
}
