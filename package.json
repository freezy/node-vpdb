	"nyc": {
		"check-coverage": true,
		"all": true,
		"cache": true,
		"lines": 10,
		"statements": 10,
		"functions": 10,
		"branches": 10,
		"extension": [
			".ts"
		],
		"require": [
			"ts-node/register"
		],
		"include": [
			"src/**/*.ts"
		],
		"exclude": [
			"src/**/*.spec.js",
			"**/*.d.ts",
			"src/files/processor/processor.ts",
			"src/common/middleware/website.middleware.ts"
		],
		"reporter": [
			"lcov",
			"html",
			"text-summary"
		]
	}
}
