		"koa-bodyparser": "^4.2.1",
		"koa-json": "^2.0.2",
		"koa-response-time": "^2.0.0",
		"koa-router": "^7.4.0",
		"koa-send": "^5.0.0",
		"lodash": "^4.17.10",
		"logform": "^1.9.1",
		"mongoose": "^5.1.7",
		"mongoose-paginate": "^5.0.3",
		"mongoose-unique-validator": "^2.0.1",
		"nodegit": "^0.22.1",
		"nodemailer": "^4.6.7",
		"ole-doc": "^0.1.1",
		"optipng": "^1.1.0",
		"passthrough-counter": "^1.0.0",
		"path-to-regexp": "^2.2.1",
		"pngquant": "^1.3.0",
		"project-name-generator": "^2.1.5",
		"pusher": "^2.0.0",
		"randomstring": "^1.1.5",
