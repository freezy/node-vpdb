{
	"name": "vpdb",
	"version": "v2.0.0",
	"description": "Virtual Pinball Database",
	"main": "src/app.ts",
	"repository": {
		"type": "git",
		"url": "https://github.com/vpdb/server"
	},
	"keywords": [
		"visual pinball",
		"vp10",
		"vpx"
	],
	"author": "freezy <freezy@vpdb.io>",
	"license": "GPL-2.0",
	"bugs": {
		"url": "https://github.com/vpdb/server/issues"
	},
	"scripts": {
		"start": "node src/app.js",
		"serve:dev": "nodemon --config deploy/nodemon/dev.json --inspect -r ts-node/register src/app",
		"serve:test": "node --inspect -r ts-node/register src/app",
		"serve:test:nyc": "nyc --reporter=lcov --reporter=html node app.js",
		"serve:test:debug": "node --inspect app.js",
		"serve:protractor": "cross-env NODE_ENV=test APP_SETTINGS=src/config/settings-protractor.js PORT=7357 APP_NAME=local run-s test:dropdbs start",
		"build:lint": "eslint src/**/*.js",
		"build:prod": "tsc src/** ",
		"test:dropdbs": "node deploy/tools/dropdb",
		"test:rimraf": "rimraf .nyc_output coverage",
		"test:wait": "node deploy/tools/wait",
		"test:finish": "cross-env PORT=7357 node deploy/tools/finish",
		"test:mocha": "mocha --reporter spec --timeout 120000 src/**/*.spec.js",
		"test:run": "run-s test:wait test:mocha test:finish",
		"test:serve": "cross-env NODE_ENV=test APP_SETTINGS=config/settings.test.js PORT=7357 APP_NAME=local run-s test:dropdbs test:rimraf serve:test",
		"test:serve:debug": "cross-env NODE_ENV=test APP_SETTINGS=src/config/settings.test.js PORT=7357 APP_NAME=local run-s test:dropdbs test:rimraf serve:test:debug",
		"test:ci": "run-p test:serve test:run"
	},
	"engines": {
		"node": "8.x"
	},
	"mod-project-name": "vpdb",
	"dependencies": {
		"@slack/client": "^4.2.2",
		"acl": "^0.4.11",
		"adm-zip": "^0.4.11",
		"bluebird": "^3.5.1",
		"bull": "^3.4.2",
		"busboy": "^0.2.14",
		"bytes": "^3.0.0",
		"chalk": "^2.4.1",
		"gm": "^1.23.1",
		"handlebars": "^4.0.11",
		"jwt-simple": "^0.5.1",
		"koa": "^2.5.1",
		"koa-bodyparser": "^4.2.0",
		"koa-passport": "^4.1.0",
		"koa-response-time": "^2.0.0",
		"koa-router": "^7.4.0",
		"lodash": "^4.17.10",
		"logform": "^1.6.0",
		"mongoose": "^5.0.15",
		"mongoose-paginate": "^5.0.3",
		"mongoose-unique-validator": "^2.0.1",
		"nodemailer": "^4.6.4",
		"optipng": "^1.1.0",
		"passport-github": "^1.1.0",
		"passport-google-oauth": "^1.0.0",
		"passport-oauth": "^1.0.0",
		"passthrough-counter": "^1.0.0",
		"pngquant": "^1.3.0",
		"pusher": "^2.0.0",
		"randomstring": "^1.1.5",
		"redis": "^2.8.0",
		"shortid32": "^0.1.1",
		"sqreen": "^1.18.4",
